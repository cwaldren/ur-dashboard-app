{
  "manifest_version": 2,
  "name": "UR Dashboard",
  "description": "Displays personal information from Blackboard.",
  "version": "0.1",
  "permissions": [
    "*://my.rochester.edu/*",
    "*://ecard.sequoiars.com/*",
    "storage",
    "management"
  ],
  "background": {
    "scripts": ["js/vendor/angular.min.js"]
  },
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  }
}